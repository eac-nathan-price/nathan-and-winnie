{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nathan-and-winnie/ant": ["libs/nw/feature-ant/src/index.ts"],
      "@nathan-and-winnie/buzzer": ["libs/nw/feature-buzzer/src/index.ts"],
      "@nathan-and-winnie/cardify": ["libs/nw/feature-cardify/src/index.ts"],
      "@nathan-and-winnie/contact": ["libs/nw/feature-contact/src/index.ts"],
      "@nathan-and-winnie/countdown": [
        "libs/nw/feature-countdown/src/index.ts"
      ],
      "@nathan-and-winnie/dice": ["libs/nw/feature-dice/src/index.ts"],
      "@nathan-and-winnie/feature-toolbar": [
        "libs/shared/feature-toolbar/src/index.ts"
      ],
      "@nathan-and-winnie/feud": ["libs/nw/feature-feud/src/index.ts"],
      "@nathan-and-winnie/forewarned": [
        "libs/nw/feature-forewarned/src/index.ts"
      ],
      "@nathan-and-winnie/pdfeedback": [
        "libs/nw/feature-pdfeedback/src/index.ts"
      ],
      "@nathan-and-winnie/peg": ["libs/nw/feature-peg/src/index.ts"],
      "@nathan-and-winnie/recipes": ["libs/nw/feature-recipes/src/index.ts"],
      "@nathan-and-winnie/rewind": ["libs/nw/feature-rewind/src/index.ts"],
      "@nathan-and-winnie/ricky": ["libs/nw/feature-ricky/src/index.ts"],
      "@nathan-and-winnie/scrabble": ["libs/nw/feature-scrabble/src/index.ts"],
      "@nathan-and-winnie/sim": ["libs/nw/feature-sim/src/index.ts"],
      "@nathan-and-winnie/sudoku": ["libs/nw/feature-sudoku/src/index.ts"],
      "@nathan-and-winnie/taskmaster": [
        "libs/nw/feature-taskmaster/src/index.ts"
      ],
      "@nathan-and-winnie/ui-window": ["libs/shared/ui-window/src/index.ts"]
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", "tmp"]
}
